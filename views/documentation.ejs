	<script type="text/javascript">
		$(function () {
			// Set the scroll range that triggers affix
			$("#sidebar").affix({
				offset: { 
					top: ($('#content').position().top-70),
					bottom: $('#footer').outerHeight()
				}
			});

			$('body').scrollspy({ target: '#sidebar' });

			$('#sidebar').on('activate.bs.scrollspy', function () {
				var href = $('#sidebar .active:first > a').attr('href');
				$('#sidebar').scrollTo($('a[href='+href+']'), 100);
			});
		});
	</script>

